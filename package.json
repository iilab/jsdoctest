{
  "name": "jsdoctest",
  "version": "1.7.0",
  "description": "Run jsdoc examples as doctests",
  "main": "lib/index.js",
  "bin": "bin/jsdoctest",
  "scripts": {
    "bin": "./bin/jsdoctest",
    "test": "JSDOCTEST_DISABLE=true nyc mocha && bash run-file-tests",
    "coverage": "JSDOCTEST_DISABLE=true nyc report --reporter=lcov mocha"
  },
  "pre-push": [
    "test",
    "coverage"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/yamadapc/jsdoctest.git"
  },
  "keywords": [
    "test",
    "jsdoc",
    "doctests",
    "testing",
    "workflow"
  ],
  "author": "Pedro Tacla Yamada <tacla.yamada@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yamadapc/jsdoctest/issues"
  },
  "homepage": "https://github.com/yamadapc/jsdoctest",
  "dependencies": {
    "commander": "^2.8.1",
    "dox": "^0.8",
    "js-beautify": "^1.5.4",
    "lodash": "^4.0.1",
    "multimatch": "^2.1.0",
    "uglifyjs": "^2.4.11"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.9.0",
    "blanket": "1.1.7",
    "bluebird": "^3.2.1",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "mocha": "^3.4.1",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^10.3.2",
    "pre-push": "^0.1.1",
    "should": "^10.0.0"
  },
  "config": {
    "blanket": {
      "data-cover-never": [
        "node_modules"
      ],
      "pattern": [
        "lib",
        "bin"
      ],
      "spec-cov": {
        "threshold": 70,
        "lcovOutput": "lcov"
      }
    }
  }
}
